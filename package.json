{
  "name": "graphql-mocker",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Chrome extension to capture, edit, and mock GraphQL responses in real-time",
  "author": "wtdlee",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtdlee/graphql-mocker"
  },
  "keywords": [
    "graphql",
    "mock",
    "mocker",
    "chrome-extension",
    "devtools",
    "testing",
    "interceptor"
  ],
  "scripts": {
    "dev": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "test": "jest",
    "zip": "zip -r dist.zip dist",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "@types/react": "^16.14.22",
    "@types/react-dom": "^16.9.14",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^8.3.0",
    "typescript": "^4.5.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.3.3"
  }
}
